<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane fx:id="employee" prefHeight="371.0" prefWidth="322.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.wkn.client.windows.controllers.EmployeeWindowController">
    <children>
        <GridPane layoutY="2.0" prefHeight="371.0" prefWidth="322.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="102.0" minWidth="10.0" prefWidth="54.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="10.0" prefWidth="215.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="98.0" minWidth="10.0" prefWidth="49.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="57.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="72.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="111.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="115.0" minHeight="10.0" prefHeight="44.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="116.0" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="114.0" minHeight="10.0" prefHeight="114.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitClick" prefHeight="25.0"
                        prefWidth="224.0" text="Выход" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Button fx:id="aboutMeButton" mnemonicParsing="false" onAction="#aboutMeClick" prefHeight="25.0"
                        prefWidth="224.0" text="Обо мне" GridPane.columnIndex="1"/>
                <Button fx:id="createEventButton" mnemonicParsing="false" onAction="#createEventClick" prefHeight="25.0"
                        prefWidth="224.0" text="Создать событие" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Button fx:id="calendarEventsButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="224.0"
                        text="Календарь событий" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Button fx:id="myTasksButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="224.0"
                        text="Мои задачи" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Button fx:id="searchEmployeeButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="224.0"
                        text="Найти сотрудника" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
